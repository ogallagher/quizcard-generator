{
    "compilerOptions": {
        "target": "es2020",
        "allowJs": false,
        "module": "commonjs",
        "sourceMap": true,
        "strict": false,
        "forceConsistentCasingInFileNames": true,
        "noEmit": false,
        "incremental": true,
        "moduleResolution": "node",
        "esModuleInterop": false,
        "declaration": true
    },
    "exclude": [
        "node_modules"
    ]
}
